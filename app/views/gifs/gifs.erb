<form action="/gifs/new" method="post" enctype="multipart/form-data">
  <label>Upload GIF:</label>
  <input type="file" name="gif">
  <label for="description">Description: </label>
  <input type="text" id="description" name="description">
  <input type="submit" value="submit">
</form>
<a href="/">Index</a> | <a href="/signout">Sign Out</a>

<% @gifs.each do |gif|%>
<img src="<%= @gif.filename %>" alt="<%= @gif.slug %>">
<!-- NEED to add tags too -->
<% end %>